#include<stdio.h>
void main(){
    int n,temp ,rev=0,r;
    printf("enter a number:");
    scanf("%d",&n);
    temp=n;
    while(n!=0){
        r=n%10;
        rev=rev*10+r;
        n=n/10;
    }
    if(rev==temp)
        printf("the number is palindrome");
    else
        printf("the number is not palindrome");
    
}
